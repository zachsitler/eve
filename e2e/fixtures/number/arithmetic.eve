print(1 + 1); // expect: 2
print(1 + -1); // expect: 0
print(1 - 1); // expect: 0
print(1 - (-1)); // expect: 2

print(2 * 2); // expect: 4
print(2 * 2 * 2); // expect: 8
print(2 * 2 + 1); // expect: 5
print(2 * (2 + 1)); // expect: 6

print(2 / 2); // expect: 1
print(2 / 4); // expect: 0.5
print((2 + 2) / 4); // expect: 1
print(2 + 2 / 2); // expect: 3
